<canvas height="<%=@map.row_number%> * 200" width=" <% @map.column_number %> * 200"></canvas>
  
<table style="width:0%">
  <tr>
    <% row = 1 %>
    <% @placements.each do |placement| %>
    <% if placement.row == row %>
      <%= render partial: "tiles", locals: {placement: placement} %>
      <% else %>
        </tr>
        <tr>
      <%= render partial: "tiles", locals: {placement: placement} %>
        <% row += 1 %>
      <% end %>
    <% end %>
   </tr> 
</table>
<svg xmlns="http://www.w3.org/2000/svg" version="1.1" height="200" width="200">
  <rect height="200" width="200" color="black">
  </rect>
</svg>
