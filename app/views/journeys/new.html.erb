<div class="transparent">
  <div class="journey">
    
  <h1>It began as all journeys do...</h1>

  <%= form_tag '/journeys', method: :post do %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <br>
    <%= label_tag :description %>
    <%= text_field_tag :description %>
    <br>
    <%= submit_tag "Submit" %>
  <% end %>
  </div>
</div>