  <% if @map.user.id == current_user.id %>    
   <th id="dropzone", ondrop="drop(event)", ondragover="allowDrop(event)"> <%= link_to  (image_tag "#{placement.tile.path}.png", {class: "tiles", id: "#{placement.id}", draggable: true, ondragstart: "drag(event)"}), "/placements/#{placement.id}/edit"%></th>
  <% elsif current_user.id == @participations.user_id && @visibility.find_by(placement_id: placement.id)%>
   <th><%= image_tag "#{placement.tile.path}.png", {class: "tiles"} %></th>
  <% else %>
    <th><%= form_tag "/visibilities/create", method: :post do %>
      <%= hidden_field_tag :placement_id, placement.id %>
      <%= hidden_field_tag :participation_id, @participations.id %>
      <%= image_submit_tag "black.png", {class: "tiles"}%></th>
    <% end %>
  <% end %>